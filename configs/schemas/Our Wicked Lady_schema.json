{
  "name": "Event Details Extraction",
  "baseSelector": "div.crawl4ai-result > div.dice_events > div.fNMGhp",
  "fields": [
    {
      "name": "image",
      "selector": "div.sc-65dfe515-1 > div.sc-65dfe515-2.Efixv img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "div.sc-65dfe515-1 > div.sc-65dfe515-2.etijBu > span.event-date",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "div.sc-65dfe515-1 > div.sc-65dfe515-2.etijBu > span.event-time",
      "type": "text"
    },
    {
      "name": "endTime",
      "selector": "div.sc-65dfe515-1 > div.sc-65dfe515-2.etijBu > span.event-time",
      "type": "text",
      "regex": {
        "pattern": ".* - (.*)",
        "group": 1
      }
    },
    {
      "name": "title",
      "selector": "div.sc-65dfe515-1 > div.sc-65dfe515-2.etijBu > h3.event-title",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.sc-65dfe515-1 > div.sc-65dfe515-2.etijBu > p.event-description",
      "type": "text"
    }
  ]
}