{
  "name": "Event Schedule",
  "baseSelector": "li.event",
  "fields": [
    {
      "name": "image",
      "selector": "span.details img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "div.end-date > div.day, div.end-date > div.month, div.end-date > div.year",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "span.time",
      "type": "text"
    },
    {
      "name": "endTime",
      "selector": "span.time",
      "type": "text"
    },
    {
      "name": "title",
      "selector": "h3[data-fontsize='16']",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "span.details p",
      "type": "text"
    }
  ]
}