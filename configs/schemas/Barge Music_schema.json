{
  "name": "Event Schedule",
  "baseSelector": "div.tribe_events",
  "fields": [
    {
      "name": "image",
      "selector": "div.tribe-events-event img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "div.tribe-events-event",
      "type": "text",
      "regex": "data-tribejson.*?dateDisplay\":\"(.*?)\""
    },
    {
      "name": "startTime",
      "selector": "div.tribe-events-event",
      "type": "text",
      "regex": "data-tribejson.*?startTime\":\"(.*?)\""
    },
    {
      "name": "endTime",
      "selector": "div.tribe-events-event",
      "type": "text",
      "regex": "data-tribejson.*?endTime\":\"(.*?)\""
    },
    {
      "name": "title",
      "selector": "h3.tribe-events-month-event-title > a",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.tribe-events-event",
      "type": "text",
      "regex": "data-tribejson.*?excerpt\":\"(.*?)\""
    }
  ]
}