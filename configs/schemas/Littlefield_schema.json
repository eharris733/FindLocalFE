{
  "name": "Event Data Extraction",
  "baseSelector": "article.wfea-venue__event",
  "fields": [
    {
      "name": "image",
      "selector": "figure > a > img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "time.wfea-venue__head-date > time.eaw-time",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "time.wfea-venue__date-time.eaw-time",
      "type": "text"
    },
    {
      "name": "endTime",
      "selector": "time.wfea-venue__date-time.eaw-time",
      "type": "regex",
      "regex": "(\\d{1,2}:[0-9]{2} [ap]{1}[m]{1})$"
    },
    {
      "name": "title",
      "selector": "h2.wfea-venue__title > a",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.wfea-venue__excerpt",
      "type": "text"
    }
  ]
}