{
  "name": "Event Calendar",
  "baseSelector": "div.tribe-events-calendar-list__event-row",
  "fields": [
    {
      "name": "image",
      "selector": "div.tribe-events-calendar-list__event-featured-image-wrapper img.tribe-events-calendar-list__event-featured-image",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "div.tribe-events-calendar-list__event-date-tag time.tribe-events-calendar-list__event-date-tag-datetime",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "div.tribe-events-calendar-list__event-datetime-wrapper time.tribe-events-calendar-list__event-datetime span.tribe-event-date-start",
      "type": "text"
    },
    {
      "name": "endTime",
      "selector": "div.tribe-events-calendar-list__event-datetime-wrapper time.tribe-events-calendar-list__event-datetime span.tribe-event-date-start",
      "type": "regex",
      "regex": ".*@ (\\d{1,2}:[0-9]{2} [ap]{1}[m]{1}).*",
      "default": "N/A"
    },
    {
      "name": "title",
      "selector": "h3.tribe-events-calendar-list__event-title a.tribe-events-calendar-list__event-title-link",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.tribe-events-calendar-list__event-description",
      "type": "text"
    }
  ]
}