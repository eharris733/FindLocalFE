{
  "name": "Event Catalog",
  "baseSelector": "div.event-card",
  "fields": [
    {
      "name": "image",
      "selector": "img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "p.date",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "p.description",
      "type": "text",
      "regex": "(\\d{1,2}:\\d{2}\\s?[ap]m)"
    },
    {
      "name": "endTime",
      "selector": "p.description",
      "type": "text",
      "regex": "-\\s?(\\d{1,2}:\\d{2}\\s?[ap]m)"
    },
    {
      "name": "title",
      "selector": "p.title",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "p.description",
      "type": "text"
    }
  ]
}