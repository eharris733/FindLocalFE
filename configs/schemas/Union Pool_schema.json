{
  "name": "Event Details Extraction",
  "baseSelector": "div.fNMGhp",
  "fields": [
    {
      "name": "image",
      "selector": "div.ueLMi img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "div.ueLMi .event-date",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "div.ueLMi .event-time",
      "type": "text"
    },
    {
      "name": "endTime",
      "selector": "div.ueLMi .event-time",
      "type": "text",
      "regex": {
        "pattern": ".* - (.*)",
        "group": 1
      }
    },
    {
      "name": "title",
      "selector": "div.ueLMi .event-title",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.ueLMi .event-description",
      "type": "text"
    }
  ]
}