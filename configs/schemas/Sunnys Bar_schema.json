{
  "name": "Event Details",
  "baseSelector": "div.pincard",
  "fields": [
    {
      "name": "image",
      "selector": "div.pincard__imageSection a img",
      "type": "attribute",
      "attribute": "src",
      "transform": "lambda x: 'https:' + x if not x.startswith('https:') else x"
    },
    {
      "name": "eventDate",
      "selector": "div.d-when.pincard__main__when",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "div.d-when.pincard__main__when",
      "type": "text",
      "regex": "(\\d{1,2}:\\d{2}(?:am|pm))"
    },
    {
      "name": "endTime",
      "selector": "div.d-when.pincard__main__when",
      "type": "text",
      "regex": "-\\s*(\\d{1,2}:\\d{2}(?:am|pm))"
    },
    {
      "name": "title",
      "selector": "h3.d-title a",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.d-text.d-text--preview.pincard__main__preview",
      "type": "text",
      "default": "N/A"
    }
  ]
}