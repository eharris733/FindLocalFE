{
  "name": "Event Details Extraction",
  "baseSelector": "div.MuiGrid-root.MuiGrid-container.MuiGrid-direction-xs-row",
  "fields": [
    {
      "name": "image",
      "selector": "div.MuiGrid-root.MuiGrid-direction-xs-row img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "h3.MuiTypography-root.MuiTypography-labelLarge",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "h3.MuiTypography-root.MuiTypography-labelLarge",
      "type": "text",
      "regex": {
        "pattern": "^(.*?)(\\d{1,2}:[0-9]{2} [ap]m)",
        "group": 2
      }
    },
    {
      "name": "endTime",
      "selector": "h3.MuiTypography-root.MuiTypography-labelLarge",
      "type": "text",
      "regex": {
        "pattern": "^(.*?)(\\d{1,2}:[0-9]{2} [ap]m) - (\\d{1,2}:[0-9]{2} [ap]m)$",
        "group": 3
      }
    },
    {
      "name": "title",
      "selector": "h3.MuiTypography-root.MuiTypography-labelLarge",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.MuiGrid-root.MuiGrid-direction-xs-row div.MuiTypography-root.MuiTypography-bodySmall",
      "type": "text"
    }
  ]
}