{
  "name": "Event Listings",
  "baseSelector": "div[data-testid='inline-content']",
  "fields": [
    {
      "name": "image",
      "selector": "div[data-testid='linkElement'] > wow-image > img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "div[data-testid='richTextElement'] > h5 > span:nth-child(1)",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "div[data-testid='richTextElement'] > h5 > span:nth-child(1)",
      "type": "text",
      "regex": {
        "pattern": "(\\d{1,2}:\\d{2} [ap]m)",
        "group": 0
      }
    },
    {
      "name": "endTime",
      "selector": "div[data-testid='richTextElement'] > h5 > span:nth-child(1)",
      "type": "text",
      "regex": {
        "pattern": "- (\\d{1,2}:\\d{2} [ap]m)",
        "group": 1
      }
    },
    {
      "name": "title",
      "selector": "div[data-testid='richTextElement'] > h5 > span:nth-child(1)",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div[data-testid='richTextElement'] > h5 > span:nth-child(n+2)",
      "type": "text"
    }
  ]
}