{
  "name": "Event Listing",
  "baseSelector": "div.eventItem",
  "fields": [
    {
      "name": "image",
      "selector": "div.thumb > a > img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "div.date[aria-label]",
      "type": "attribute",
      "attribute": "aria-label"
    },
    {
      "name": "startTime",
      "selector": "div.time",
      "type": "text",
      "regex": {
        "pattern": "Show: (\\d{1,2}:\\d{2} [APM]{2})",
        "group": 1
      }
    },
    {
      "name": "endTime",
      "selector": "div.time",
      "type": "text",
      "regex": {
        "pattern": "Show: \\d{1,2}:\\d{2} [APM]{2}.*?/(\\s*\\d{1,2}:\\d{2} [APM]{2})",
        "group": 1
      }
    },
    {
      "name": "title",
      "selector": "h3.title > a",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "h4.tagline",
      "type": "text",
      "default": "N/A"
    }
  ]
}