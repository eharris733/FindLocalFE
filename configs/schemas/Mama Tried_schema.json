{
  "name": "Upcoming Events",
  "baseSelector": "article.eventlist-event",
  "fields": [
    {
      "name": "image",
      "selector": "a.eventlist-column-thumbnail > img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "eventDate",
      "selector": "ul.eventlist-meta > li.eventlist-meta-date > time.event-date",
      "type": "text"
    },
    {
      "name": "startTime",
      "selector": "ul.eventlist-meta > li.eventlist-meta-time > span.event-time-localized > time.event-time-localized-start",
      "type": "text"
    },
    {
      "name": "endTime",
      "selector": "ul.eventlist-meta > li.eventlist-meta-time > span.event-time-localized > time.event-time-localized-end",
      "type": "text"
    },
    {
      "name": "title",
      "selector": "h1.eventlist-title > a.eventlist-title-link",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.eventlist-description",
      "type": "text"
    }
  ]
}